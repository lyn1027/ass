INCLUDE Irvine32.inc

.data
studentID   BYTE "학번: 2023664016", 0     ; 0은 문자열의 끝(NULL)을 의미
studentName BYTE "이름: 김채린", 0

.code
main PROC
    ; 1. 학번 출력
    MOV   EDX, OFFSET studentID   ; EDX에 studentID 문자열의 주소를 로드
    CALL  WriteString             ; EDX가 가리키는 문자열을 출력
    CALL  Crlf                    ; 줄바꿈 (Carriage Return, Line Feed)

    ; 2. 이름 출력
    MOV   EDX, OFFSET studentName   ; EDX에 studentName 문자열의 주소를 로드
    CALL  WriteString             ; EDX가 가리키는 문자열을 출력
    CALL  Crlf                    ; 줄바꿈
    CALL  Crlf                    ; (출력 예시처럼 한 줄 더 띄우기)

    ; 3. 레지스터 값 설정 (출력할 값들을 미리 세팅)
    MOV   EAX, 1234               ; EAX 레지스터에 1234 저장
    MOV   EBX, 5678               ; EBX 레지스터에 5678 저장
    MOV   ECX, 9012               ; ECX 레지스터에 9012 저장
    MOV   EDX, 4218931            ; EDX 레지스터에 4218931 저장
                                  ; (바로 위에서 WriteString에 썼던 EDX 값은 덮어쓰임)

    ; 4. 모든 범용 레지스터의 현재 값 출력
    CALL  DumpRegs                ; EAX, EBX, ECX, EDX 등의 현재 값을 화면에 출력

    ; 5. 프로그램 종료
    exit                          ; Irvine32의 종료 매크로

main ENDP

END main
